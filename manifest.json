{
  "name": "Random Websites",
  "version": "1.0",
  "description": "An extension to take you to random websites",
  "manifest_version": 3,
  "action": {"default_popup": "main.html"},
  "icons": {
    "128": "favicon.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["data.json"],
      "matches": ["http://*/*"]
    }
  ],
  "background": {
    "service_worker": "content.js"
  }
}