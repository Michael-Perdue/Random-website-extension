{
  "name": "Random Websites",
  "version": "1",
  "description": "Base Level Extension",
  "manifest_version": 3,
  "action": {"default_popup": "main.html"},
  "icons": {
    "128": "favicon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["data.json"],
      "matches": ["http://*/*"]
    }
  ],
  "background": {
    "service_worker": "popup.js"
  },
  "permissions": ["tabs","scripting","activeTab"]
}